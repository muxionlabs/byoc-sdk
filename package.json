{
  "name": "@muxionlabs/byoc-sdk",
  "version": "1.0.5",
  "description": "Production-quality SDK for Livepeer BYOC video streaming with AI processing",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "prepublishOnly": "npm run build",
    "demo:dev": "cd demo && npm install && npm run dev",
    "demo:build": "cd demo && npm install && npm run build",
    "demo:preview": "cd demo && npm run preview",
    "serve:examples": "npx http-server . -o examples/html-demo.html -p 3005"
  },
  "keywords": [
    "livepeer",
    "byoc",
    "webrtc",
    "whip",
    "whep",
    "streaming",
    "ai",
    "video"
  ],
  "author": "Livepeer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muxionlabs/byoc-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/muxionlabs/byoc-sdk/issues"
  },
  "homepage": "https://github.com/muxionlabs/byoc-sdk#readme",
  "engines": {
    "node": ">=20"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jsdom": "^26.1.0",
    "react": "^18.3.1",
    "tsup": "^8.0.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
